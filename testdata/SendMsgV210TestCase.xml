<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
     xmlns="http://www.springframework.org/schema/beans"  
     xsi:schemaLocation="http://www.springframework.org/schema/beans 
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Task -->
	<bean id="Task" class="main.java.com.interf.eyee.entity.TaskEntity">
		<property name="testCase">
			<list>
				<ref bean="TestCase1" />
				<ref bean="TestCase2" />
				<ref bean="TestCase3" />
			</list>
		</property>
	</bean>
	<!-- TestCase1 -->
	<bean id="TestCase1" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>0</value>
		</property>
		<property name="name">
			<value>使用错误格式的手机号发送手机验证码</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/User/SendMsgV210</value>
		</property>
		<property name="input">
			<ref bean="Input1" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine1" />
		</property>
	</bean>
	<bean id="Input1" class="main.java.com.interf.eyee.entity.forcase.SendMsgV210InputEntity">
		<property name="mobile">
			<value>17740887033</value>
		</property>
		<property name="type">
			<value>1</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine1" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511572</value>
		</property>
		<property name="msg">
			<value>手机号码格式不正确</value>
		</property>
		<property name="data">
			<null/>
		</property>
	</bean>
	<!-- TestCase2 -->
	<bean id="TestCase2" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name">
			<value>使用未注册的手机号发送手机验证码</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/User/SendMsgV210</value>
		</property>
		<property name="input">
			<ref bean="Input2" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine2" />
		</property>
	</bean>
	<bean id="Input2" class="main.java.com.interf.eyee.entity.forcase.SendMsgV210InputEntity">
		<property name="mobile">
			<value>+86-15001799381</value>
		</property>
		<property name="type">
			<value>1</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine2" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511549</value>
		</property>
		<property name="msg">
			<value>您的手机号码未注册，请先注册</value>
		</property>
		<property name="data">
			<null/>
		</property>
	</bean>
	<!-- TestCase3 -->
	<bean id="TestCase3" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>2</value>
		</property>
		<property name="name">
			<value>使用空的type发送手机验证码</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/User/SendMsgV210</value>
		</property>
		<property name="input">
			<ref bean="Input3" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine3" />
		</property>
	</bean>
	<bean id="Input3" class="main.java.com.interf.eyee.entity.forcase.SendMsgV210InputEntity">
		<property name="mobile">
			<value>17740887033</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine3" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511503</value>
		</property>
		<property name="msg">
			<value>缺少必要的参数</value>
		</property>
		<property name="data">
			<null/>
		</property>
	</bean>
	<!-- AssertUtil -->
	<bean id="AssertUtil" class="main.java.com.interf.eyee.utils.assertutils.NormalAssertUtil" />
</beans>