<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
     xmlns="http://www.springframework.org/schema/beans"  
     xsi:schemaLocation="http://www.springframework.org/schema/beans 
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Task -->
	<bean id="Task" class="main.java.com.interf.eyee.entity.TaskEntity">
		<property name="testCase">
			<list>
				<ref bean="TestCase1" />
				<ref bean="TestCase2" />
				<ref bean="TestCase3" />
			</list>
		</property>
	</bean>
	<!-- TestCase1 -->
	<bean id="TestCase1" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>0</value>
		</property>
		<property name="name">
			<value>使用正确的必填信息获取所有类目</value>
		</property>
		<property name="assertType">
			<value>1</value>
		</property>
		<property name="api">
			<value>api/Product/NewSearchV218</value>
		</property>
		<property name="input">
			<ref bean="Input1" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine1" />
		</property>
	</bean>
	<bean id="Input1" class="main.java.com.interf.eyee.entity.forcase.NewSearchV218InputEntity">
		<property name="issearchall">
			<value>1</value>
		</property>
		<property name="pageindex">
			<value>1</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine1" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511200</value>
		</property>
		<property name="msg">
			<value>请求已完成</value>
		</property>
		<property name="data">
			<null/>
		</property>
	</bean>
	<bean id="Data1" class="main.java.com.interf.eyee.entity.forcase.NewSearchV218DataEntity">
		
	</bean>
	<!-- TestCase2 -->
	<bean id="TestCase2" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name">
			<value>使用空的pageindex获取所有类目</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/Product/NewSearchV218</value>
		</property>
		<property name="input">
			<ref bean="Input2" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine2" />
		</property>
	</bean>
	<bean id="Input2" class="main.java.com.interf.eyee.entity.forcase.NewSearchV218InputEntity">
		<property name="issearchall">
			<value>1</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine2" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511504</value>
		</property>
		<property name="msg">
			<value>不合法的参数值</value>
		</property>
		<property name="data">
			<value>页码，是否搜全部，排序方式，排序规则</value>
		</property>
	</bean>
	<!-- TestCase3 -->
	<bean id="TestCase3" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>2</value>
		</property>
		<property name="name">
			<value>使用空的issearchall获取所有类目</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/Product/NewSearchV218</value>
		</property>
		<property name="input">
			<ref bean="Input3" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine3" />
		</property>
	</bean>
	<bean id="Input3" class="main.java.com.interf.eyee.entity.forcase.NewSearchV218InputEntity">
		<property name="pageindex">
			<value>1</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine3" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511503</value>
		</property>
		<property name="msg">
			<value>缺少必要的参数</value>
		</property>
		<property name="data">
			<value>缺少搜索条件</value>
		</property>
	</bean>
	<!-- AssertUtil -->
	<bean id="AssertUtil" class="main.java.com.interf.eyee.utils.assertutils.NormalAssertUtil" />
</beans>