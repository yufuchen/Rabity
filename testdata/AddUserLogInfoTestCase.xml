<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
     xmlns="http://www.springframework.org/schema/beans"  
     xsi:schemaLocation="http://www.springframework.org/schema/beans 
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Task -->
	<bean id="Task" class="main.java.com.interf.eyee.entity.TaskEntity">
		<property name="testCase">
			<list>
				<ref bean="TestCase1" />
				<ref bean="TestCase2" />
				<ref bean="TestCase3" />
			</list>
		</property>
	</bean>
	<!-- TestCase1 -->
	<bean id="TestCase1" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>0</value>
		</property>
		<property name="name">
			<value>使用正确的信息增加用户loginfo</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/Business/AddUserLogInfo</value>
		</property>
		<property name="input">
			<ref bean="Input1" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine1" />
		</property>
	</bean>
	<bean id="Input1" class="main.java.com.interf.eyee.entity.forcase.AddUserLogInfoInputEntity">
		<property name="content">
			<value>{"log: device_id":"b8ccd9578ece423e80230e1bebf886d1","log: tag":"2","log: version":"2.2.5","log: entapp":"1"}</value>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine1" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511200</value>
		</property>
		<property name="msg">
			<value>请求已完成</value>
		</property>
		<property name="data">
			<value>succeed</value>
		</property>
	</bean>
	<!-- TestCase2 -->
	<bean id="TestCase2" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name">
			<value>使用空的cotent增加用户loginfo</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/Business/AddUserLogInfo</value>
		</property>
		<property name="input">
			<ref bean="Input2" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine2" />
		</property>
	</bean>
	<bean id="Input2" class="main.java.com.interf.eyee.entity.forcase.AddUserLogInfoInputEntity">
		<property name="content">
			<null/>
		</property>
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine2" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511504</value>
		</property>
		<property name="msg">
			<value>不合法的参数值</value>
		</property>
		<property name="data">
			<value>缺少必要的参数:Content</value>
		</property>
	</bean>
	<!-- TestCase3 -->
	<bean id="TestCase3" class="main.java.com.interf.eyee.entity.TestCaseEntity">
		<property name="id">
			<value>2</value>
		</property>
		<property name="name">
			<value>不使用cotent增加用户loginfo</value>
		</property>
		<property name="assertType">
			<value>0</value>
		</property>
		<property name="api">
			<value>api/Business/AddUserLogInfo</value>
		</property>
		<property name="input">
			<ref bean="Input3" />
		</property>
		<property name="baseLine">
			<ref bean="BaseLine3" />
		</property>
	</bean>
	<bean id="Input3" class="main.java.com.interf.eyee.entity.forcase.AddUserLogInfoInputEntity">
		<property name="param">
			<value/>
		</property>
	</bean>
	<bean id="BaseLine3" class="main.java.com.interf.eyee.entity.BaseLineEntity">
		<property name="code">
			<value>1511504</value>
		</property>
		<property name="msg">
			<value>不合法的参数值</value>
		</property>
		<property name="data">
			<value>缺少必要的参数:Content</value>
		</property>
	</bean>
	<!-- AssertUtil -->
	<bean id="AssertUtil" class="main.java.com.interf.eyee.utils.assertutils.NormalAssertUtil" />
</beans>